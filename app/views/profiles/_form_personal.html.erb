<%= form_for @profile do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>
  <%= f.label :address %>
  <%= f.text_field :address %><br>
  <%= f.label :date_of_birth %>
  <%= f.text_field :date_of_birth %><br>
  <%= f.label :contact %>
  <%= f.text_field :contact %><br>
  <%= f.label :gender %>
  <%= f.text_field :gender %><br>
  <%= f.label :nickname %>
  <%= f.text_field :nickname %><br>
  <%= f.label :payment_method, t(:receive_method)%>
  <%= f.text_field :payment_method %><br>
  <%= f.label :work_document %>
  <%= f.text_field :work_document %><br>
  <%= f.submit t(:submit_btn) %>
<% end %>
