<%= form_for @profile do |f| %>
  <div class="row">
    <div class="col form-group">
      <%= f.label :first_name, 'Nome', class: "col-form-label" %>
      <%= f.text_field :first_name, class: "form-control" %>
    </div>

    <div class="col form-group">
      <%= f.label :last_name, 'Sobrenome', class: "col-form-label" %>
      <%= f.text_field :last_name, class: "form-control" %>
    </div>
  </div>

  <div class="row">
    <div class="col form-group">
      <%= f.label :document, 'Documento', class: "col-form-label" %>
      <%= f.text_field :document, class: "form-control" %>
    </div>

    <div class="col form-group">
      <%= f.label :nickname, 'Apelido', class: "col-form-label" %>
      <%= f.text_field :nickname, class: "form-control" %>
    </div>
  </div>

  <div class="row">
    <div class="col form-group">
      <%= f.label :date_of_birth, 'Data de Nascimento', class: "col-form-label" %>
      <%= f.date_field :date_of_birth, class: "form-control" %>
    </div>

    <div class="col form-group">
      <%= f.label :gender, 'Sexo', class: "col-form-label" %>
      <%= f.text_field :gender, class: "form-control" %>
    </div>
  </div>

  <div class="row">
    <div class="col form-group">
      <%= f.label :address, 'EndereÃ§o', class: "col-form-label" %>
      <%= f.text_field :address, class: "form-control" %>
    </div>

    <div class="col form-group">
      <%= f.label :payment_method, 'Metodo de pagamento', class: "col-form-label" %>
      <%= f.text_field :payment_method, class: "form-control" %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :contact, 'Contato', class: "col-form-label" %>
    <%= f.text_field :contact, class: "form-control" %>
  </div>

  <div class="form-group text-center">
    <%= f.submit 'Enviar', class: "btn btn-black btn-sm" %>
  </div>
<% end %>